    <rect width="85" height="65" x="-20" y="10" fill="white"/>
    <!-- Organization structure -->
    <path d="M50 30 L50 40 M30 50 L70 50 M30 50 L30 60 M50 50 L50 60 M70 50 L70 60"
          stroke="currentColor"
          stroke-width="2"
          stroke-linecap="round"
          fill="none"
    />
