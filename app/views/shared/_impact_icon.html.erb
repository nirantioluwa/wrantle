<%# Impact Icon %>
<div class="icon-container">
  <svg class="impact-icon" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
    <!-- White background rectangle -->
    <rect width="85" height="65" x="-20" y="10" fill="white"/>
    
    <!-- Ripple effect -->
    <circle cx="50" cy="50" r="10" 
            stroke="#1a7f37"
            stroke-width="2"
            fill="none"/>
    <circle cx="50" cy="50" r="20" 
            stroke="#1a7f37"
            stroke-width="1.5"
            fill="none"
            stroke-dasharray="2 2"/>
    <circle cx="50" cy="50" r="30" 
            stroke="#1a7f37"
            stroke-width="1"
            fill="none"
            stroke-dasharray="1 3"/>
    
    <!-- Central point -->
    <circle cx="50" cy="50" r="4" 
            fill="#1a7f37"/>
  </svg>
</div>
