<div class="icon-container">
    <svg class="tech-ip-icon" viewBox="0 0 100 100" fill="none" xmlns="http://www.w3.org/2000/svg">
      <!-- White background rectangle -->
      <rect width="85" height="65" x="-20" y="10" fill="white"/>
      
      <!-- Circuit board pattern -->
      <path d="M30 50 H45 M55 50 H70
               M50 30 V45 M50 55 V70
               M30 50 V70 M70 50 V70
               M40 70 H60"
            stroke="#1a7f37"
            stroke-width="1.5"
            stroke-linecap="round"
            stroke-dasharray="1 3"/>
      
      <!-- Connection nodes -->
      <circle cx="50" cy="50" r="3" 
              fill="#1a7f37"/>
      <circle cx="30" cy="70" r="2" 
              fill="#1a7f37"/>
      <circle cx="50" cy="70" r="2" 
              fill="#1a7f37"/>
      <circle cx="70" cy="70" r="2" 
              fill="#1a7f37"/>
              
      <!-- IP Shield outline -->
      <path d="M35 25 H65 L50 20 Z
               M35 25 Q50 45, 50 75
               M65 25 Q50 45, 50 75"
            stroke="#1a7f37"
            stroke-width="2"
            fill="none"
            stroke-linecap="round"
            stroke-linejoin="round"
            pathLength="1"
            stroke-dasharray="1 2 3 2 1 3 2"/>
    </svg>
</div>
